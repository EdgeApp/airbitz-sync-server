{
  "name": "airbitz-sync-server",
  "version": "1.0.0",
  "description": "Git server wrapper",
  "main": "none.js",
  "scripts": {
    "build": "flow-remove-types src/ -d lib/ && flow-remove-types testsrc/ -d test/",
    "flow": "flow",
    "lint:fix": "standard '*.js' 'src/**/*.js' 'testsrc/**/*.js' --fix",
    "lint": "standard '*.js' 'src/**/*.js' 'testsrc/**/*.js' && npm run flow",
    "test": "npm run lint",
    "precommit": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "Paul Puey <paul@edgesecure.co",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.49.1",
    "flow-remove-types": "^1.2.3",
    "husky": "^0.14.3",
    "mocha": "^3.1.2",
    "standard": "^10.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "dependencies": {
    "forever": "^0.15.3",
    "mkdirp": "^0.5.1",
    "nano": "^6.3.0",
    "rsync": "^0.6.1",
    "sprintf-js": "^1.1.1",
    "sync-request": "4.0.1"
  }
}
